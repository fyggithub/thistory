#include "Board.h"
/********************************************************************
 *
 *******************************************************************/
VOID LED1Set(UINT8 v)
{
	Gpio* led = BoardGetGpio (IO_TESTLED);
	GpioSet (led, v);
}
/********************************************************************
 *
 *******************************************************************/
VOID TimerTest(VOID)
{
    STATIC UINT8 STATUS=0;
    STATUS = (STATUS == 0)? 1:0;
    LED1Set(STATUS);
}

VOID LEDtest(UINT8 s)
{
	if(s==0)
		LED1Set(s);
	else
		LED1Set(1);
}
